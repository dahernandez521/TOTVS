			EndIf
			//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
			//Ё Verifica os campos C6_PRCVEN, C6_VALOR e C6_PRUNIT se estao em branco|
			//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
			If ( lRetorna .And. AT(M->C5_TIPO,"CIP")==0 )
				If ( Empty(aCols[nCntFor,nPPrcven]) ) .or. ( Empty(aCols[nCntFor,nPValor]))
					//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
					//Tratamento para quando for valor do item igual a zero permitido quando F4_VLRZERO = SIMЁ
					//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
					If !(lF4VlZero .And. Posicione("SF4",1,xFilial("SF4")+aCols[n][nPTes],"SF4->F4_VLRZERO") == "1") 
						Help(" ",1,"A410VZ")
						lRetorna := .F.
					EndIf	
				EndIf
			EndIf
			If ( lRetorna .And. AT(M->C5_TIPO,"CIP") <> 0 )
				If ( Empty(aCols[nCntFor,nPPrcven]) ) .or. ( Empty(aCols[nCntFor,nPValor]) )
					Help(" ",1,"A410VZ2")
					lRetorna := .F.
				EndIf
			EndIf
			
			If SC5->(FieldPos("C5_TPCOMPL")) > 0 .And. ( lRetorna .And. AT(M->C5_TIPO,"C") <> 0 )
				If M->C5_TPCOMPL == "1" 	//Compl. PreГo
