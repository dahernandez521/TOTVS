						//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
						If ( aColsGrade[nCntFor][nLinha][nColuna][1] <> 0 .And.;
								If(lTestaDel,!aColsOrig[nCntFor][Len(aHeader)+1],.T.) )
