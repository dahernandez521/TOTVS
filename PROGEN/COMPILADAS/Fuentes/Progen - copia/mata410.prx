Local lRet := .F.
Default cCondPAdt:="0"
#IFDEF TOP
	If cPaisLoc $ "ANG|BRA|MEX|PER" .AND. !Empty(cCondPagto) .AND. FindFunction("FPedAdtGrv") .and. AliasInDic("FIE") //jgr
		DbSelectArea("SE4")
		DbSetOrder(1)
