		If !lWmsNew .and. nOpcA == 1 .AND. SC6->(FieldPos("C6_PEDCOM")) > 0 .AND.  SC6->(FieldPos("C6_ITPC")) > 0 .AND. SC6->(FieldPos("C6_FILPED")) > 0  
			A410CCPed(aCols,aHeader,aMTA177PER,2)   
		EndIf
		
