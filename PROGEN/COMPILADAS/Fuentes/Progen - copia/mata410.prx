			aSF1[nAT,1] := oMarked
		Else
			Aviso(OemToAnsi(STR0014),STR0176,{"OK"})	//"Aten┤└o"###"Esta Nota Fiscal estА sendo utilizada por outro usuАrio e nЦo poderА ser selecionada."
			SF1->(RestArea(aAreaSF1))
		EndIf
	ElseIf aSF1[nAT,1] == oMarked
		 //Faz o desbloqueio do registro que foi bloqueado pelo SimpleLock().
		SF1->(RecLock("SF1"))
		SF1->(MsUnlock())	
		aSF1[nAT,1] := oNoMarked
	EndIf
EndIf

Return


