Function A410Line(nAT,aSF1)

Local abLine     := {}
Local nCnt       := 0

For nCnt := 1 To Len(aSF1[nAT])
	If nCnt == 1
		Aadd( abLine, Iif(aSF1[ nAT, nCnt ] , oMarked, oNoMarked ) )
	Else
		Aadd( abLine, aSF1[ nAT, nCnt ] )
	EndIf
Next nCnt

Return abLine

