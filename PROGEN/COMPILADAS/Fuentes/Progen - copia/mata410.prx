If SX3->(dbSeek("C6_OPER")) .AND. !('Vazio() .Or. ExistCpo("SX5","DJ"+M->C6_OPER) .And. A410SitTrib() .AND. MTA410OPER(n)' $ AllTrim(SX3->X3_VALID))
