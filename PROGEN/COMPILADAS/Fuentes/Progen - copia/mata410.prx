								aCols[NX][nPCLVL]   := Space(Len(aCols[NX][nPCLVL]))
							EndIf
							
						EndIf
					Next nX
				EndIf
			EndIf
		Else
			If nItem > 0
				aColsAGG[nItem][2]	:= aClone(aColsCC)
			Else
				aadd(aColsAGG,{aCols[N][nPItemNF],aClone(aColsCC)})
			EndIf
			aCols[nSavN][nPRateio] := "2"
		EndIf
	Else
		If nTotPerc > 0 .And. nItem > 0
			If "C6_RATEIO" $ cCampo
				&cCampo := "1"
			EndIf
			aCols[nSavN][nPRateio] := "1"
