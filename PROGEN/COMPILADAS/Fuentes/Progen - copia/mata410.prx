		If (Type("INCLUI") <> "U" .AND. Type("ALTERA") <> "U")
			lContinua := !(INCLUI.OR.ALTERA)
		endif

		If (SE4->E4_TIPO=="9".AND.(lContinua .OR. lRent)) ;
			.OR. SE4->E4_TIPO<>"9"
		
			If SFB->FB_JNS == 'J' .And. cPaisLoc == 'COL'
				 dbSelectArea("SFC")
