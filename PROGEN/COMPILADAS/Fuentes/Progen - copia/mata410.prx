			//Ё Finaliza a gravacao dos lancamentos do SIGAPCO            Ё
			//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
			PcoFinLan("000100")

		Else
			aAutoCab := MsAuto2Ench("SC5")
			aAutoItens := MsAuto2Gd(aHeader,aCols)
		EndIf
	EndIf
